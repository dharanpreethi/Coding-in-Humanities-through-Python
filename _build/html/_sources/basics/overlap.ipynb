   ],
   "source": [
    "class Person:\n",
    "\n",
    "    def name(self):\n",
    "        print(\"person\")\n",
    "\n",
    "class Employee(Person):\n",
    "\n",
    "    def name(self):\n",
    "        super().name()\n",
    "\n",
    "        print(\"employee\")\n",
    "\n",
    "y = Employee()\n",
    "y.name()\n",
    "\n"
   ]
